<aura:component implements="flexipage:availableForAllPageTypes,forceCommunity:availableForAllPageTypes" access="global">
    <aura:attribute name="isMainScreen" type="Boolean" default="True"/>
    <aura:attribute name="selectedResource" type="B25__Resource__c"/>
    
    <aura:if isTrue="{!v.isMainScreen}">
        <c:resourceTileList selectedResource="{!v.selectedResource}" isMainScreen="{!v.isMainScreen}"></c:resourceTileList>
        <aura:set attribute="else">
            <c:BookingCalendarFLT selectedResource="{!v.selectedResource}" isMainScreen="{!v.isMainScreen}"></c:BookingCalendarFLT>
        </aura:set>
    </aura:if>
</aura:component>