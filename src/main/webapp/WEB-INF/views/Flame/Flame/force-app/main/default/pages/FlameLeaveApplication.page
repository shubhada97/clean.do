    <script>
    
    function getDate()
    {
        var x = document.querySelector('[id$="check"]').checked;
        
        if(x)
        {
            document.querySelector('[id$="toDate"]').value = document.querySelector('[id$="fromDate"]').value;
            document.querySelector('[id$="toDate"]').disabled = true;
        }
        else{
            document.querySelector('[id$="toDate"]').disabled = false;
        }
        cpLeaveValidate();
    }
    
    function changedate()
    {
        var y = document.querySelector('[id$="check"]').checked;
        
        if(y){
            document.querySelector('[id$="toDate"]').value = document.querySelector('[id$="fromDate"]').value;
        }
        else{
            document.querySelector('[id$="toDate"]').disabled = false;
        }
        cpLeaveValidate();
        validateToDate();
    }
    
