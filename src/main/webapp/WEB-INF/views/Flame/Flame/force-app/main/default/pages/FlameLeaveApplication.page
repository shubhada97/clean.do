    function validateToDate(){
        //console.log('Validate to Date function');
        
        var wrkFromdate = document.querySelector('[id$="fromDate"]').value;
        var wrkTodate = document.querySelector('[id$="toDate"]').value;
        //console.log('wrkdate'+wrkdate);
        
        
        var date1 = $.datepicker.parseDate('dd/mm/yy', wrkFromdate);
        var date2 = $.datepicker.parseDate('dd/mm/yy', wrkTodate);
        
        
        if(date1 > date2){
            $.alert('To date can not be less than From date.');
            //document.querySelector('[id$="toDate"]').value = wrkFromdate;
        }
        else{
            //console.log('in past');
        }
        cpLeaveValidate();
    }
    
    
    //validate compoff date
    function validateDate(){
        
        var currentDate1 = moment().subtract(1, 'days').format('DD/MM/YYYY');
        
        var leavetype = document.querySelector('[id$="Leave_Ledger"]').value;
        var wrkdate = document.querySelector('[id$="fromDate2"]').value;
        
        
        var date1 = $.datepicker.parseDate('dd/mm/yy', wrkdate);
        var date2 = $.datepicker.parseDate('dd/mm/yy', currentDate1);
        
        
