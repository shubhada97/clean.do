    /****** function to check page condition when it loads and hide or show some elements ***********/
    pageConditions:function(){
       
        
        $('#outIndia').hide();
        $('.passport-dtls').hide();
        $('.diff-address').hide();
        $('#fatLastCmp').hide();
        $('#fatCurrentCmp').hide();
        $('#fotDesignation').hide();
        $('#motLastCmp').hide();
        $('#motCurrentCmp').hide();
        $('#motDesignation').hide();
        $('#country').hide();
        $('#otherID').hide();
        $('#otherPostal').hide();
        if($('#fatherOccu input[type="radio"]:checked').val()===undefined){
            $('#fatLastCmp').hide();
            $('#fatCurrentCmp').hide();
            $('#fatDesignation').hide();
        }
        if($('#mailingCity input[type="radio"]:checked').val()=="Different"){
            if($('#mailAddCity select').val()!=="India" && $('#mailAddCity select').val()!==""){
              $('#mailPostal').show();  
            }    
        }
        var a= $('#sourceID select').val();
        if(a=="Other"){
            $('#otherID').show();
        }
        var b=$('#otherCity select').val();
        if(b=="Other"){
            $('#otherPostal').show();
        }
        var c=$('#mailCity select').val();
        if(c == "Other"){
            $('#mailPostal').show();
        }else{
            $('#mailPostal').hide();
        }  
        if($('#nationality [type="radio"]:checked').val()=="Yes"){
            $('.passport-dtls').hide();
            $('#country').hide();
            $('#state').show();
            if($('#state select').val()=="Outside India"){
                $('#outIndia').show();
            }
            }else{
            $('.passport-dtls').show();
            $('#country').show();
            $('#state').hide();
        } 
        var hi = $('#mailingCity [type="radio"]:checked').val();
        if(hi=="Different"){
            $('.diff-address').show();
            if($('#mailAddCity select').val()!=="India" && $('#mailAddCity select').val()!==""){
                $('#mailPostal').show();  
            }  
        }else{
            $('.diff-address').hide();
        }
        var abc = $('#fatherOccu [type="radio"]:checked').val();
        switch(abc){
            case "Retired":
                $('#fatLastCmp').show();
                $('#fatEmail').show();
                $('#fatMobile').show();
                $('#fatCurrentCmp').hide();
                $('#fatDesignation').show();
            break;
            case "Working":
                $('#fatLastCmp').hide();
                $('#fatCurrentCmp').show();
                $('#fatDesignation').show();
                $('#fatEmail').show();
                $('#fatMobile').show();
            break;
            case "Homemaker":
                $('#fatLastCmp').hide();
                $('#fatCurrentCmp').hide();
                $('#fatDesignation').hide();
                $('#fatEmail').show();
                $('#fatMobile').show();
            break;
            case "Not Applicable":
                $('#fatEmail').hide();
                $('#fatMobile').hide();
                $('#fatLastCmp').hide();
                $('#fatCurrentCmp').hide();
                $('#fatDesignation').hide();
            break;
        }    
        var motherDes= $('#motherOccu [type="radio"]:checked').val();
        switch(motherDes){
            case "Retired":
                $('#motMobile').show();
                $('#motLastCmp').show();
                $('#motCurrentCmp').hide();
                $('#motDesignation').show();
            break;
            case "Working":
                $('#motMobile').show();
                $('#motLastCmp').hide();
                $('#motCurrentCmp').show();
                $('#motDesignation').show();
            break;
            case "Homemaker":
                $('#motMobile').show();
                $('#motLastCmp').hide();
                $('#motCurrentCmp').hide();
                $('#motDesignation').hide();
            break;
            case "Not Applicable":
                $('#motEmail').hide();
                $('#motMobile').hide();
                $('#motLastCmp').hide();
                $('#motCurrentCmp').hide();
                $('#motDesignation').hide();
            break;        
        } 
        if($('#perAddrss select').val()!="India" && $('#perAddrss select').val()!==""){ 
            $('#otherPostal').show()
        }
        if($('#mailAddCity select').val()!="India" && $('#mailAddCity select').val()!==""){ 
            $('#mailPostal').show()
        }
        
