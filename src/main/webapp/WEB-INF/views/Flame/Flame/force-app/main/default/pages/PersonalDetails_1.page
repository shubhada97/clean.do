    /****** function to validate email ***********/
    emailValidation:function(em){
        intRegex = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
        var emailflag = intRegex.test(em);
        if (!emailflag) {
           return false;
        }else{
            return true;
        }
    },
    /****** function to format Date accordint to salesforce format ***********/
    dateFormate:function(calendar){
        var ctDate=calendar.val();
        var bDate="";
        if(ctDate!==""){
            console.log('new date',ctDate);
            var c=ctDate.split("-");
            bDate=new Date(c[2]+"-"+c[1]+"-"+c[0]);
            console.log('before   ',bDate);
            var day=bDate.getDate();
            var month=bDate.getMonth()+1;
            var year=bDate.getFullYear();
            bDate=year+"-"+month+"-"+day;
            console.log(bDate); 
        }
        return bDate;
    },
