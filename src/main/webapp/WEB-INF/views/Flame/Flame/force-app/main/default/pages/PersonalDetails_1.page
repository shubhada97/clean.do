    /********* Attached Bootstrap Calender to inputs *******/
    addCalender:function(){
        var n = new Date();
        var currentYear  = n.getFullYear();
       

        if($("[id$='datePicker1']").val() ==""){
            var futYearSet = currentYear + 10;
            $("[id$='datePicker1']").datepicker({
                format: 'dd/mm/yyyy',
                startDate: n,
                autoclose: true               
            });
            $("[id$='datePicker1']").datepicker('setDate', new Date(futYearSet, 01, 01));
            $("[id$='datePicker1']").datepicker('update');
             $("[id$='datePicker1']").val('');
        }else{
             $("[id$='datePicker1']").datepicker({
                format: 'dd/mm/yyyy',
                startDate: n,
                autoclose: true               
            });
        }
        if($("[id$='datePicker2']").val() == ""){
             var preYearSet = currentYear - 25;
            $("[id$='datePicker2']").datepicker({                                                          
               format:'dd/mm/yyyy',
               endDate: n,
               autoclose: true
            });
            $("[id$='datePicker2']").datepicker('setDate', new Date(preYearSet, 01, 01));
           $("[id$='datePicker2']").datepicker('update');
             $("[id$='datePicker2']").val('');
        }else{
            $("[id$='datePicker2']").datepicker({                                                          
               format:'dd/mm/yyyy',
                 endDate: n,
               autoclose: true
            });
        }
      // $("[id$='datePicker2']").datepicker('startDate','-9125d');
