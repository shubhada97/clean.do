     $(document).on('change',"#programId select",function(){
     //  alert($("#programId select option:eq(0)").val() );
       var ProgramName = $('#programId select').find(":selected").val();
       var ProgramType = '{!ProgramType}' ;
      // alert('{!ProgramType}');
       //if( $("#programId select option:eq(0)").val() == ProgramName){
       if(ProgramType == 'Postgraduate'){
           pgMBA();
           initiatListener();
        }
       
      //  if($(this).val() == "Postgraduate (MBA)"){
      //  if( $("#programId select option:eq(1)").val() == ProgramName){
        if(ProgramType == 'Doctorate'){
           pgMBA();
           initiatListener();
        }
        //if($(this).val() == "Postgraduate (MBA [Communications Management])"){
        //if( $("#programId select option:eq(2)").val() == ProgramName){
        if(ProgramType == 'Postgraduate'){
           pgMBAC();
           initiatListener();
          
        }
        //if($(this).val() == "Undergraduate (B.A.,BBA,B.Sc.)"){
        //if( $("#programId select option:eq(3)").val() == ProgramName){
        if(ProgramType == 'Undergraduate'){
            ugBA();
            initiatListener();
            
        }
        if(ProgramType == 'Other UG'){
            Oug();
            initiatListener();
            
        }
        if(ProgramType == 'Other UG'){
            Opg();
            initiatListener();
            
        }
        
    });
     
  $(document).ready(function(){
