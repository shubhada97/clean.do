        function runOnEnter(ev) 
        { 
            if (window.event && window.event.keyCode == 13 || ev.which == 13) 
            { 
                movetopersonaldetails(); 
                return false; 
            } else 
            { 
                return true; 
            } 
        } 
        
        function addRemoveExam(currentElement,selectedExam)
        {  
            $('#loader-overlay').show();
                console.log('show me');
                setTimeout(function()
            {
                console.log('hide me');
                $('#loader-overlay').hide();
                initiatListener(); 
                $('.chkPrgMsg').text('');
            },1000);        
            if(currentElement.checked) 
            {                    
                console.log("add --"+selectedExam );
                addExamAction(selectedExam);
            }
            else
            {    
                console.log("remove --"+selectedExam );
                RemoveCompetitionExams(selectedExam);
            }
        }
            
     function changeSelection(){
         var a =$('#gdId select');
         var len = a.length;
         for(var i=0;i<len;i++){
             a[i].selectedIndex = 0;
         }
         changeChecked();
         removeCopy();
     }
     function changeChecked(){
         $('#satChk input[type="checkbox"]').prop('checked',false);     
     }
     function pgMBA(){
          $('.first-heading').show();
            $('#prgCourses').show();
            $('#satChk').hide();
            $('#satChk1').hide();
            $('#actChk').hide();
            $('#othChk').hide();
            $('.sec-heading').hide();
            $('#satId').hide();
            $('#gdId').hide();
            changeSelection();            
     }
     function pgMBAC(){
        $('.first-heading').show();
        $('#prgCourses').show();
        $('#satChk').hide();
        $('#satChk1').hide();
        $('#actChk').hide();
        $('#othChk').hide();
        $('.sec-heading').hide();
         $('#satId').hide();
         $('#gdId').hide();
         changeSelection();                  
     }
     function ugBA(){
        $('.first-heading').hide();
        $('#prgCourses').hide();            
        $('#satChk').show();
        $('#satChk1').show();
        $('#actChk').show();
        $('#othChk').hide();
